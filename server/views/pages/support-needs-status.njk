{% extends "../partials/layout.njk" %}
{% set pathway = "Resettlement overview" %}
{% set pageTitle = applicationName + " - " + pathway + " Support Needs" %}
{% set mainClasses = "app-container govuk-body" %}
{% set prisonerNumber = prisonerData.personalDetails.prisonerNumber %}
{% set supportNeedsEnabled = features.SUPPORT_NEEDS | getFeatureFlag %}

{% block header %}
  {% if feComponents.header %}
    {{ feComponents.header | safe }}
  {% else %}
    {% include "../partials/header.njk" %}
  {% endif %}
  <div class="govuk-width-container">
    {{
      govukPhaseBanner({
      tag: {
      text: phaseName
      },
      html: feedbackText
      })
    }}
  </div>
{% endblock %}

{% block content %}
  <div class="govuk-grid-row">
    <div class="govuk-grid-column-three-quarters">
      <h1 class="govuk-heading-l">Support needs status</h1>
    </div>
  </div>
{% endblock %}

