{% macro preReleaseComplete(pathwayName, prisonerData) %}
<div class="govuk-grid-column-two-thirds">
  <div class="govuk-notification-banner--success govuk-notification-banner" role="region" aria-labelledby="govuk-notification-banner-title" data-module="govuk-notification-banner">
    <div class="govuk-notification-banner__header">
      <h2 class="govuk-notification-banner__title" id="govuk-notification-banner-title">Important</h2>
    </div>
    <div class="govuk-notification-banner__content">
      <p class="govuk-notification-banner__heading">Pre-release report completed.<br /><br />
        <p>To amend the pre-release report view each pathway and change answers as required. </p>
      </p>
    </div>
  </div>
</div>
{% endmacro %}
